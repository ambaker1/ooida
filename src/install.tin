package require tin 1.0
tin add -auto vutil https://github.com/ambaker1/vutil install.tcl
tin add -auto ndlist https://github.com/ambaker1/ndlist install.tcl
tin add -auto mpjobs https://github.com/ambaker1/mpjobs install.tcl
tin depend vutil @VUTIL_VERSION@
tin depend ndlist @NDLIST_VERSION@
tin depend mpjobs @MPJOBS_VERSION@
set dir [tin mkdir -force ooida @VERSION@]
file copy LICENSE README.md pkgIndex.tcl ooida.tcl $dir
